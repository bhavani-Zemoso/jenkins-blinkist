    "jest": "^28.1.1",
    "jest-sonar-reporter": "^2.0.0",
    "prop-types": "^15.8.1",
    "react-hot-loader": "^4.13.0",
    "sonar-scanner": "^3.1.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.2"
